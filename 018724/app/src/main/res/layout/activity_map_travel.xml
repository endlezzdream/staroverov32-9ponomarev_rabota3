<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".map_travel">

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="244dp"
        android:layout_marginBottom="16dp"
        android:text="Дата поездки:04.01.2025"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/UW6R"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Рим, Италия Колизей: Один из самых известных символов Рима, древний амфитеатр, где проходили гладиаторские бои."
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="260dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Милан, Италия Миланский собор (Дуомо): Величественный готический собор, с которого открывается прекрасный вид на город."
        tools:layout_editor_absoluteY="317dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Марсель, Франция Старый порт (Vieux Port): Сердце города, где можно насладиться атмосферой и попробовать свежие морепродукты."
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="388dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Барселона, Испания Саграда Фамилия: Уникальный собор, спроектированный Антонио Гауди. Парк Гуэль: Красивый парк с мозаичными скамейками и архитектурными элементами Гауди."
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="467dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Малага, Испания Крепость Алькасаба: Мавританская крепость с великолепными видами на город."
        tools:layout_editor_absoluteY="561dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Марокко Марракеш: Исследуйте медину, рынок и знаменитый сад Мажорель."
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="613dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/uw6r"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="-9dp" />

</androidx.constraintlayout.widget.ConstraintLayout>